{
  "family": "Custom-server-taskdefinition",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "Custom-server-container",
      "image": "$IMAGE_NAME",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "2048",
  "executionRoleArn": "test",
  "taskRoleArn": "test"
}